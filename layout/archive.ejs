<div id="archive">
  <h2>Topic Tags</h2>
  <p><a href='/tags/data-science/'>#data-science</a> <a href='/tags/software-development/'>#software-development</a> <a href='/tags/public-policy/'>#public-policy</a> <a href='/tags/civic-action/'>#civic-action</a> <a href='/tags/economics/'>#economics</a> <a href='/tags/book-review/'>#book-review</a> </p>
  <ul class="post-list">
    <% var year = 0 %>
    <% var change = false %>
    <% page.posts.sort('date', 'desc').each(function(post) { %>
      <% var itemYear = date(post.date, 'YYYY') - 0 %>
      <% change = year !== itemYear %>
      <% year = change ? itemYear : year %>
      <% if (change) { %>
        <h2><%= year %></h2>
      <% } %>
      <li class="post-item">
        <%- partial('_partial/post/date', { post: post, class_name: 'meta' }) %>
        <span><%- partial('_partial/post/title', { post: post, index: true, class_name: '' }) %><span>
      </li>
    <% }); %>
  </ul>
  <%- partial('_partial/pagination') %>
</div>
